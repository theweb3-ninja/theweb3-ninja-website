{
  "name": "eveo-marketplace-app",
  "private": true,
  "version": "2.0.0",
  "type": "module",
  "scripts": {
    "dev": "react-router dev",
    "build": "react-router build && node scripts/update-security-txt.cjs",
    "build:staging": "react-router build --mode=staging && node scripts/update-security-txt.cjs",
    "start": "react-router-serve ./dist/server/index.js",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit --project tsconfig.app.json && tsc --noEmit --project tsconfig.node.json && tsc --noEmit --project tsconfig.scripts.json && tsc --noEmit --project tsconfig.cloudflare.json",
    "cf-dev": "CLOUDFLARE_WRANGLER=true wrangler dev --local --live-reload",
    "cf-typegen": "CLOUDFLARE_WRANGLER=true wrangler types",
    "cf-build": "CLOUDFLARE_WRANGLER=true wrangler build --env production",
    "cf-deploy": "CLOUDFLARE_WRANGLER=true wrangler deploy --env production",
    "cf-deploy:staging": "CLOUDFLARE_WRANGLER=true wrangler deploy --env staging",
    "deploy": "bun run build && bun run cf-deploy",
    "deploy:staging": "bun run build:staging && wrangler versions upload --env staging",
    "check-format": "prettier --check \"**/*.{ts,tsx,js,jsx,js,css,json,md}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,js,css,json,md}\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/primitive": "^1.1.3",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-compose-refs": "^1.1.2",
    "@radix-ui/react-context": "^1.1.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-icons": "^1.3.2",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-presence": "^1.1.5",
    "@radix-ui/react-primitive": "^2.1.3",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@radix-ui/react-use-controllable-state": "^1.2.2",
    "@radix-ui/react-use-previous": "^1.1.1",
    "@radix-ui/react-use-size": "^1.1.1",
    "@tanstack/react-query": "^5.85.3",
    "class-variance-authority": "^0.7.1",
    "country-flag-icons": "^1.5.19",
    "date-fns": "^4.1.0",
    "dayjs": "^1.11.13",
    "embla-carousel": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "eslint-config-prettier": "^10.1.8",
    "i18next": "^25.3.6",
    "i18next-browser-languagedetector": "^8.2.0",
    "isbot": "^5.1.30",
    "react": "^19.1.1",
    "react-cookie-consent": "^9.0.0",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.6.1",
    "react-router": "^7.8.1",
    "recharts": "^3.1.2",
    "zod": "^4.0.17",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@cloudflare/vite-plugin": "^1.11.6",
    "@eslint/js": "^9.33.0",
    "@forge42/seo-tools": "^1.4.5",
    "@react-router/dev": "^7.8.1",
    "@react-router/node": "^7.8.1",
    "@react-router/serve": "^7.8.1",
    "@tailwindcss/postcss": "^4.1.12",
    "@tailwindcss/vite": "^4.1.12",
    "@testing-library/jest-dom": "^6.7.0",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/eslint__js": "^9.14.0",
    "@types/jest": "^30.0.0",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.3.0",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@types/testing-library__jest-dom": "^6.0.0",
    "autoprefixer": "^10.4.21",
